{
  "name": "commerce-webapi",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdobeDocs/commerce-webapi"
  },
  "dependencies": {
    "@adobe/gatsby-theme-aio": "4.6.6",
    "gatsby": "4.22.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "remark-cli": "^10.0.1",
    "remark-validate-links": "^11.0.2",
    "spectaql": "^1.5.6"
  },
  "scripts": {
    "start": "gatsby build && gatsby serve",
    "start:prefix": "gatsby build --prefix-paths && gatsby serve --prefix-paths",
    "dev": "gatsby develop",
    "dev:https": "gatsby develop --https --host localhost.corp.adobe.com --port 9000",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:links": "remark src/pages --quiet --frail",
    "generate:graphql": "spectaql --one-file --target-dir static/ --target-file graphql-api.html spectaql/config.yml"
  },
  "remarkConfig": {
    "plugins": [
      "remark-validate-links"
    ]
  },
  "packageManager": "yarn@3.2.4"
}
